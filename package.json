{
  "name": "vikepress",
  "version": "0.0.23",
  "exports": {
    ".": "./src/index.ts",
    "./client/initFeatureList": {
      "browser": "./src/components/features/initFeatureList.ts"
    }
  },
  "scripts": {
    "// VikePress Demo": "",
    "dev": "pnpm run build && pnpm run demo:dev",
    "preview": "pnpm run build && pnpm run demo:preview",
    "demo:dev": "vikepress dev",
    "demo:preview": "vikepress build && vikepress preview",
    "// VikePress": "",
    "build": "tsup ./cli/ --format esm --out-dir ./dist/cli/",
    "prepublishOnly": "pnpm run build",
    "reset": "git clean -Xdf && pnpm install"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.0.0",
    "@mdx-js/react": "^2.0.0",
    "@mdx-js/rollup": "^2.0.0",
    "@vitejs/plugin-react": "^1.0.4",
    "express": "^4.17.1",
    "rehype-pretty-code": "^0.3.2",
    "remark-gfm": "^3.0.1",
    "shiki": "^0.10.1",
    "twemoji": "^13.1.0",
    "vite": "^2.9.10",
    "vite-plugin-glob": "0.2.8",
    "vite-plugin-ssr": "0.4.0-beta.38"
  },
  "peerDependencies": {
    "react": "18",
    "react-dom": "18"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rollup": "^2.74.1",
    "tsup": "^6.0.1",
    "typescript": "^4.5.4",
    "vikepress": "link:"
  },
  "bin": {
    "vikepress": "./cli.mjs"
  },
  "files": [
    "src/*",
    "*.ts",
    "dist/*",
    "cli.mjs",
    "vite.config.ts",
    "vite.config/*"
  ],
  "type": "module"
}
