{
  "name": "@brillout/docpress",
  "version": "0.1.15",
  "exports": {
    "./renderer/_default.page.server.js": "./dist/renderer/_default.page.server.js",
    "./renderer/_default.page.client.js": "./dist/renderer/_default.page.client.js",
    "./renderer/_default.page.server.css": "./dist/renderer/_default.page.server.css",
    "./renderer/_default.page.client.css": "./dist/renderer/_default.page.client.css",
    ".": "./dist/index.js",
    "./features/FeatureList": "./dist/components/features/FeatureList.js",
    "./features/FeatureList.css": {
      "browser": "./dist/components/features/FeatureList.css"
    },
    "./features/initFeatureList": {
      "browser": "./dist/components/features/initFeatureList.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ],
      "mdx": [
        "mdx.d.ts"
      ],
      "features/*": [
        "dist/components/features/*"
      ]
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "rm -rf dist/ && framework-builder",
    "prepublishOnly": "pnpm run build",
    "release": "npm version patch && npm publish && git push && git push --tags"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.0.0",
    "@mdx-js/react": "^2.0.0",
    "@mdx-js/rollup": "^2.0.0",
    "@vitejs/plugin-react": "^2.0.1",
    "balloon-css": "^1.2.0",
    "express": "^4.17.1",
    "rehype-pretty-code": "^0.3.2",
    "remark-gfm": "^3.0.1",
    "shiki": "^0.10.1",
    "twemoji": "^13.1.0",
    "vite": "^3.0.9",
    "vite-plugin-ssr": "^0.4.54"
  },
  "peerDependencies": {
    "react": "18",
    "react-dom": "18"
  },
  "devDependencies": {
    "@brillout/framework-builder": "^0.0.2",
    "@types/express": "^4.17.13",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.6",
    "docpress": "link:",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rollup": "^2.74.1",
    "tsx": "^3.12.1",
    "typescript": "^4.8.4"
  },
  "bin": {
    "docpress": "./dist/cli/index.js"
  },
  "files": [
    "mdx.d.ts",
    "dist/"
  ],
  "type": "module",
  "license": "MIT",
  "repository": "https://github.com/brillout/docpress",
  "publishConfig": {
    "access": "public"
