    "@mdx-js/rollup": "^2.0.0",
    "@vitejs/plugin-react": "^1.0.4",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-pretty-code": "^0.2.2",
    "remark-gfm": "^3.0.1",
    "shiki": "^0.10.1",
    "twemoji": "^13.1.0",
    "vite": "^2.8.4",
    "vite-plugin-ssr": "^0.4.0-beta.28"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.6",
    "rollup": "^2.74.1",
    "tsup": "^6.0.1",
    "typescript": "^4.5.4",
    "vikepress": "link:"
