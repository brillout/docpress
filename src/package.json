{
  "name": "@brillout/docpress",
  "version": "0.5.39",
  "scripts": {
    "// Check types while developing": "",
    "types": "tsc --noEmit --watch",
    "// Develop Docpress using demo/": "",
    "dev": "pnpm run dev:build && pnpm run dev:start",
    "dev:build": "tsup",
    "dev:start": "node dist/index.js dev",
    "// Build Docpress": "",
    "build": "rimraf dist/ && framework-builder",
    "// Preview build": "",
    "preview": "pnpm run preview:build && pnpm run preview:start",
    "preview:build": "pnpm run build && node dist/cli/index.js build",
    "preview:start": "node dist/cli/index.js preview",
    "// Test": "",
    "test": "test-e2e",
    "// Release": "",
    "release": "release-me patch",
    "release:breaking-change": "release-me minor"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.0.0",
    "@mdx-js/react": "^2.0.0",
    "@mdx-js/rollup": "^2.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "balloon-css": "^1.2.0",
    "express": "^4.17.1",
    "rehype-pretty-code": "^0.3.2",
    "remark-gfm": "^3.0.1",
    "shiki": "^0.10.1",
    "twemoji": "^13.1.0",
    "vike": "^0.4.158",
    "vike-contributors": "^0.0.8",
    "vite": "^4.3.9"
  },
  "type": "module",
  "exports": {
    "./renderer/_default.page.server.tsx": "./renderer/_default.page.server.tsx",
    "./renderer/_default.page.client.ts": "./renderer/_default.page.client.ts",
    ".": "./index.ts",
    "./features/FeatureList": "./components/features/FeatureList.tsx",
    "./features/FeatureList.css": {
      "browser": "./components/features/FeatureList.css"
    },
    "./features/initFeatureList": {
      "browser": "./components/features/initFeatureList.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*"
      ],
      "types": [
        "types.d.ts"
      ],
      "features/*": [
        "./components/features/*"
      ]
    }
  },
  "peerDependencies": {
    "react": "18",
    "react-dom": "18"
  },
  "devDependencies": {
    "@brillout/docpress": "link:.",
    "@brillout/framework-builder": "^0.1.1",
    "@brillout/release-me": "^0.1.13",
    "@brillout/test-e2e": "^0.5.25",
    "@types/express": "^4.17.13",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.6",
    "docpress": "link:",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.74.1",
    "tsup": "^7.0.0",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4"
  },
  "bin": {
    "docpress": "./bin.js"
  },
  "license": "MIT",
  "repository": "https://github.com/brillout/docpress",
  "publishConfig": {
    "access": "public"
  }
}
