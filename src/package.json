{
  "name": "@brillout/docpress",
  "version": "0.16.12",
  "type": "module",
  "scripts": {
    "======== Build": "",
    "// Build vite.config.ts and +config.ts (other files don't need to be built as @brillout/docpress is noExternal)": "",
    "build": "rm -rf dist/ && tsc --project tsconfig.config.json",
    "======== Develop": "",
    "// Typecheck + build vite.config.ts and +config.ts": "",
    "dev": "tsc --watch",
    "========= Release": "",
    "release": "release-me patch",
    "release:minor": "release-me minor",
    "release:commit": "release-me commit"
  },
  "dependencies": {
    "@brillout/picocolors": "^1.0.10",
    "@docsearch/css": "3.9.0",
    "@docsearch/react": "3.9.0",
    "@mdx-js/mdx": "3.0.1",
    "@mdx-js/react": "3.0.1",
    "@mdx-js/rollup": "3.0.1",
    "@shikijs/transformers": "1.2.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "detype": "^1.1.3",
    "npm-to-yarn": "^3.0.1",
    "rehype-pretty-code": "0.13.0",
    "remark-directive": "^4.0.0",
    "remark-gfm": "4.0.0",
    "shiki": "1.2.0",
    "unist-util-visit": "^5.0.0",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "@vitejs/plugin-react-swc": ">=3.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "typescript": ">=5.0.0",
    "vike": ">=0.4.234",
    "vite": ">=5.2.0"
  },
  "peerDependenciesMeta": {
    "@vitejs/plugin-react-swc": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "exports": {
    "./renderer/onRenderHtml": "./renderer/onRenderHtml.tsx",
    "./renderer/onRenderClient": "./renderer/onRenderClient.tsx",
    "./renderer/onCreatePageContext": "./renderer/onCreatePageContext.ts",
    ".": "./index.ts",
    "./config": "./dist/+config.js",
    "./style": "./css/index.css",
    "./types": {
      "types": "./types.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*"
      ],
      "types": [
        "./types.d.ts"
      ],
      "config": [
        "./dist/+config.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@brillout/release-me": "^0.4.8",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "mdast-util-directive": "^3.1.0",
    "mdast-util-mdx-jsx": "^3.2.0"
  },
  "repository": "https://github.com/brillout/docpress",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
