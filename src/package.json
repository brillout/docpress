{
  "name": "@brillout/docpress",
  "version": "0.5.40",
  "scripts": {
    "// Build vite.config.ts": "",
    "build": "rm -rf dist/ && tsc --project tsconfig.vite-config.json",
    "// Check types while developing": "",
    "types": "tsc --noEmit --watch",
    "// Release": "",
    "release": "release-me patch",
    "release:commit": "release-me commit",
    "release:breaking-change": "release-me minor"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.0.0",
    "@mdx-js/react": "^2.0.0",
    "@mdx-js/rollup": "^2.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "balloon-css": "^1.2.0",
    "rehype-pretty-code": "^0.3.2",
    "remark-gfm": "^3.0.1",
    "shiki": "^0.10.1",
    "twemoji": "^13.1.0",
    "vike-contributors": "^0.0.9"
  },
  "type": "module",
  "exports": {
    "./renderer/onRenderHtml": "./renderer/onRenderHtml.tsx",
    "./renderer/client": "./renderer/client.ts",
    ".": "./index.ts",
    "./features/FeatureList": "./components/features/FeatureList.tsx",
    "./features/initFeatureList": {
      "browser": "./components/features/initFeatureList.ts"
    },
    "./vite-config": "./dist/vite.config.js",
    "./style": "./css/index.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*"
      ],
      "types": [
        "./types.d.ts"
      ],
      "vite-config": [
        "./dist/vite.config.d.ts"
      ],
      "features/*": [
        "./components/features/*"
      ]
    }
  },
  "peerDependencies": {
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "*",
    "vike": "^0.4.161",
    "vite": "^4.3.9"
  },
  "devDependencies": {
    "@brillout/release-me": "^0.1.13",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vike": "^0.4.161",
    "vite": "^4.3.9",
    "typescript": "^4.9.4"
  },
  "license": "MIT",
  "repository": "https://github.com/brillout/docpress",
  "publishConfig": {
    "access": "public"
  }
}
