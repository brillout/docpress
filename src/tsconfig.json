{
  "compilerOptions": {
    "outDir": "./dist/",
    "jsx": "react",
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "Bundler",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    // "noUncheckedIndexedAccess": true,
    "strict": true
  }
}
